{
  "name": "agent-orchestrator",
  "version": "1.0.0",
  "description": "Multi-agent orchestration system for Cursor IDE integration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "agent:dev": "tsx watch src/agent-manager.ts",
    "agent:start": "node dist/agent-manager.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "validate-package": "node -e \"console.log('Package validation passed'); process.exit(0);\"",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "format-check": "prettier --check \"src/**/*.ts\"",
    "security-audit": "npm audit --audit-level=moderate",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint && npm run type-check"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "ws": "^8.14.2",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3",
    "axios": "^1.6.2",
    "socket.io-client": "^4.7.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@types/node-cron": "^3.0.11",
    "typescript": "^5.3.2",
    "tsx": "^4.6.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "prettier": "^3.1.0"
  },
  "keywords": [
    "agents",
    "orchestration",
    "cursor",
    "ide",
    "ai",
    "multi-agent",
    "typescript",
    "websocket",
    "real-time",
    "dashboard",
    "monitoring"
  ],
  "author": {
    "name": "Multi-Agent Orchestration System",
    "email": "team@multiagent.dev",
    "url": "https://github.com/your-org/multi-agent-system"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/multi-agent-system.git",
    "directory": "apps/agents"
  },
  "bugs": {
    "url": "https://github.com/your-org/multi-agent-system/issues"
  },
  "homepage": "https://github.com/your-org/multi-agent-system#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/your-org"
  }
}
